{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "chrome",
        "request": "launch",
        "name": "Debug Extension",
        "url": "https://dexscreener.com",
        "webRoot": "${workspaceFolder}",
        "sourceMaps": true,
        "pathMapping": {
          "/": "${workspaceFolder}"
        },
        "runtimeArgs": [
          "--load-extension=${workspaceFolder}"
        ],
        "userDataDir": "${workspaceFolder}/.vscode/chrome-debug-profile"
      }
    ]
  }